}

.div-list{
    /* border: 1px solid #eee; */
    height: 490px;
}

.div-list .dyq{
    margin: 0;
    height: 30px;
    background-color: #ffffff;
    color: #639bf3;
    /* margin-bottom:  10px; */
    border-left: 1px solid #69696a;
}

.div-list .dyq>span{
    display:  inline-block;
    width: 113px;
    text-align:  center;
    line-height: 28px;
    margin-right: 0px;
    font-size:  14px;
    border: 1px solid #939292;
    border-left: none;
    cursor:  pointer;
    color: #333;
    float: left;
}

.active{
    background-color: #776f6f;
    color: #fbf9f9 !important;
}

.wk .yizhu{
    margin: auto;
    overflow:auto;
    position:relative;
    height: 459px;
    display: none;
    color: #333;
    margin-top: 1px;
    border: 1px solid #eee;
}

.wk .yizhu>table{
    table-layout: fixed;
    empty-cells: show;
    border-collapse: collapse;
    margin: 0 auto;
    border: 1px solid #cad9ea;
    color: #333;
    width: 100%;
    /* height: 100%; */
    overflow:  auto;
    font-size: 12px;
}

.wk .yizhu>table>tbody{

}

.wk .yizhu>table>tbody>tr:hover{
    background-color: #e6edf3;
}

.wk .yizhu>table>tbody>tr:nth-child(2n){
    background-color: #f2f2f2;
}

.wk .yizhu>table>tbody>tr>td,.wk .yizhu>table>thead>tr>th{
   border: 1px solid #d7d7d7;
    padding: 0 5px 0;
    height: 30px;
    font-size: 12px;
    width:55px;
    font-weight:  400;
    box-sizing: border-box;
}

