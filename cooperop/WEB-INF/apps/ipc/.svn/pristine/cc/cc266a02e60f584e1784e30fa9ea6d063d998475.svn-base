.wk .box-content .box-list>p:nth-child(n+2){
    line-height: 20px;
    font-size: 14px;
    margin: 0;
}

.wk .box-content .box-list>p:nth-child(n+2)>b{
    font-weight: 300;
    font-size: 12px;
}

.wk .box-content .box-list .fstP{
    line-height:  20px;
    margin-bottom: 0;
    width: 100%;
    font-size: 14px;
    color: #000;
    position:  relative;
    margin:0;
}

.fstP span .level_1{
	
font-size: 14px;
	
color: #0bcd02;
}

.fstP span .level_2{
	
font-size: 14px;
	
color: #f5bb07;
}

.fstP span .level_3{
	
font-size: 14px;
	
color: #f77802;
}

.fstP span .level_4{
	
font-size:  14px;
	
color: #ff0000;
}



.wk .box-content .box-list .fstP>i{
    cursor:  pointer;
    top: 5px;
}

.wk .box-content .box-list .fstP>.likeup{
  
position: absolute;
  
right: 120px;
  
/* top: 14px; */
}

.wk .box-content .box-list .fstP>.hatedown{
  
position:  absolute;
  
right: 80px;
}


.wk .box-content .box-list .fstP>i.notactive{
	display: none;
}
.wk .box-content .box-list .fstP>a{
    float:  right;
    text-decoration:  underline;
    font-size:  14px;
}

.wk .box-content .box-list .fstP>a:hover{
    color:#1cccbc;
}

.wk .btn-content{
    position: absolute;
    /* height: 60px; */
    right: 0;
    /* left: 50%; */
    top: -15px;
}

.wk .btn-content a{
    float:  right;
    padding:0 10px;
    min-width: 90px;
    line-height: 40px;
    /* margin: 15px; */
    margin: 20px 20px 0px 0px;
    text-align:  center;
    font-size:  14px;
    text-decoration:  none;
    color: #fff;
    border-radius: 3px;
    background-color: #0fbbab;
    cursor: hand;
}

.wk .btn-content a:hover{
    background-color: #009688;
}

.sqmyy {
	display: block;
	cursor: hand;
}


.nav-content a{
	display: block;
	font-size: 16px;
}

.wk .box-content2{
    position: absolute;
    top: 111px;
    right: 0px;
