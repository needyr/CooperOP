*{
    margin:0;
    padding:0;
    font-style:  normal;
}
body{
	width: 100% !important;
	overflow:  hidden;
}
.wk{
    width: 100%;
    position: absolute;
    background-color:  #fff;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.wk .top-content{
    position:  absolute;
    top: 0;
    right: 0;
    left: 200px;
    /* min-height: 100px; */
    border-bottom: 1px dashed #e1dfdf;
    height: 110px;
    overflow: auto;
}

.wk .top-content .top-div{
    height:  100%;
    padding: 10px 20px;
    /* overflow: auto; */
}

.wk .top-content .top-div .top-p>a{
	font-size: 14px;
}

.wk .top-content .top-div .top-p>span{
    margin-right: 20px;
    font-weight: 400;
    font-size: 14px;
}

.wk .top-content .top-div .top-p{
    line-height: 19px;
    margin-bottom: 5px;
    font-size: 14px;
    width: calc(100% - 245px);
}

.wk .left-content{
    position:  absolute;
    top: 0;
    left: 0;
    bottom: 0;
    width: 200px;
    border-right: 1px dashed #605f5f;
}

.wk .left-content ul{
    min-height: 600px;
    list-style:  none;
    padding: 10px 10px;
    height: 100%;
    overflow: auto;
}

.wk .left-content ul li{
    line-height: 35px;
    padding:0 10px;
    font-size: 18px;
}

.wk .left-content ul li:hover{
    background-color: #ededed;
}

.wk .left-content ul .li-active{
    background-color: #ededed;
}

.wk .left-content ul li b{
    color: #009688;
}

.wk .box-content{
    position: absolute;
    top: 110px;;
    right: 0px;
    left: 200px;
    /* overflow: auto; */
    bottom: 0;
}



.wk .box-content .box-list{
    padding: 5px 30px 5px 15px;
    border-bottom: 2px dashed #e1dfdf;
    position:  relative;
}

.wk .box-content .box-list .ss_c{right:0px;position:absolute;top:0px;height:40px;width:21px;writing-mode:lr-tb;font-size:14px;border-radius:0px 0px 33px 33px !important;text-align:center;padding-top: 5px;color:#ffffff;background-color:#db7dda;border:0px #1a1717 solid;border-left:none;cursor:-webkit-grabbing;}
.wk .box-content .box-list .intercept_c{right:0px;position:absolute;top:0px;height:40px;width:21px;writing-mode:lr-tb;font-size:14px;border-radius:0px 0px 33px 33px !important;text-align:center;padding-top:5px;color:#ffffff;background-color:#e65050;border:0px #1a1717 solid;border-left:none;cursor:-webkit-grabbing;}
.wk .box-content .box-list .hint_c{right:0px;position:absolute;top:0px;height:40px;width:21px;writing-mode:lr-tb;font-size:14px;border-radius:0px 0px 33px 33px !important;text-align:center;padding-top:5px;color:#ffffff;background-color:#e65050;border:0px #1a1717 solid;border-left:none;cursor:-webkit-grabbing;}

