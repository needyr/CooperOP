    box-shadow: 4px 4px 10px 0px #848282;
    border: 1px solid #cecece;
    box-sizing: border-box;
}

.wk .box-content3{
    position: absolute;
    top: 91px;
    right: 0px;
    left: 260px;
    height: 20px;
    text-align: center;
    z-index:90;
    background-color: #f2eded;
    /* overflow: auto; */
    cursor:  pointer;
}

